{
  "name": "node-handlebars-scss-webpack",
  "version": "0.0.1",
  "private": true,
  "description": "Node with Handlebars and SCSS",
  "main": "app.js",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "scripts": {
    "dev": "node_modules/.bin/npm-run-all -p watch:server watch:build",
    "watch:server": "node_modules/.bin/nodemon",
    "server": "node app.js",
    "watch:build": "node_modules/.bin/webpack --watch --colors",
    "build": "node_modules/.bin/webpack --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjpalacios/node-handlebars-scss-webpack.git"
  },
  "keywords": [
    "handlebars",
    "webpack",
    "scss",
    "node",
    "express"
  ],
  "author": "Javi Palacios <javi@fjp.es>",
  "bugs": {
    "url": "https://github.com/fjpalacios/node-handlebars-scss-webpack/issues"
  },
  "homepage": "https://github.com/fjpalacios/node-handlebars-scss-webpack#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "jquery": "^3.3.1",
    "normalize.css": "^8.0.0",
    "popper.js": "^1.14.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "css-loader": "^0.28.11",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.1.3",
    "sass-loader": "^7.0.3",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.6"
  }
}
